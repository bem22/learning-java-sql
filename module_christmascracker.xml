<?xml version="1.0" encoding="UTF-8"?>
<project name="module_christmascracker" default="compile.module.christmascracker">
  <dirname property="module.christmascracker.basedir" file="${ant.file.module_christmascracker}"/>
  
  <property name="module.jdk.home.christmascracker" value="${project.jdk.home}"/>
  <property name="module.jdk.bin.christmascracker" value="${project.jdk.bin}"/>
  <property name="module.jdk.classpath.christmascracker" value="${project.jdk.classpath}"/>
  
  <property name="compiler.args.christmascracker" value="-encoding UTF-8 -source 1.5 -target 1.5 ${compiler.args}"/>
  
  <property name="christmascracker.output.dir" value="${module.christmascracker.basedir}/target/classes"/>
  <property name="christmascracker.testoutput.dir" value="${module.christmascracker.basedir}/target/test-classes"/>
  
  <path id="christmascracker.module.bootclasspath">
    <!-- Paths to be included in compilation bootclasspath -->
  </path>
  
  <path id="christmascracker.module.production.classpath">
    <path refid="${module.jdk.classpath.christmascracker}"/>
    <path refid="library.json-20171018.classpath"/>
    <path refid="library.maven:_com.google.code.gson:gson:2.8.2.classpath"/>
    <path refid="library.maven:_postgresql:postgresql:9.1-901.jdbc4.classpath"/>
    <path refid="library.maven:_com.squareup.okhttp3:okhttp:3.9.1.classpath"/>
    <path refid="library.maven:_com.squareup.okio:okio:1.13.0.classpath"/>
    <path refid="library.maven:_org.json:json:20171018.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-surefire-provider:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-launcher:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-engine:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.jupiter:junit-jupiter-api:5.1.0-m1.classpath"/>
    <path refid="library.maven:_org.opentest4j:opentest4j:1.0.0.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-commons:1.1.0-m1.classpath"/>
  </path>
  
  <path id="christmascracker.runtime.production.module.classpath">
    <pathelement location="${christmascracker.output.dir}"/>
    <path refid="library.json-20171018.classpath"/>
    <path refid="library.maven:_com.google.code.gson:gson:2.8.2.classpath"/>
    <path refid="library.maven:_postgresql:postgresql:9.1-901.jdbc4.classpath"/>
    <path refid="library.maven:_com.squareup.okhttp3:okhttp:3.9.1.classpath"/>
    <path refid="library.maven:_com.squareup.okio:okio:1.13.0.classpath"/>
    <path refid="library.maven:_org.json:json:20171018.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-surefire-provider:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-launcher:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-engine:1.0.2.classpath"/>
    <path refid="library.maven:_org.apache.maven.surefire:surefire-api:2.19.1.classpath"/>
    <path refid="library.maven:_org.apache.maven.surefire:common-java5:2.19.1.classpath"/>
    <path refid="library.maven:_org.junit.jupiter:junit-jupiter-api:5.1.0-m1.classpath"/>
    <path refid="library.maven:_org.opentest4j:opentest4j:1.0.0.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-commons:1.1.0-m1.classpath"/>
  </path>
  
  <path id="christmascracker.module.classpath">
    <path refid="${module.jdk.classpath.christmascracker}"/>
    <pathelement location="${christmascracker.output.dir}"/>
    <path refid="library.json-20171018.classpath"/>
    <path refid="library.maven:_com.google.code.gson:gson:2.8.2.classpath"/>
    <path refid="library.maven:_postgresql:postgresql:9.1-901.jdbc4.classpath"/>
    <path refid="library.maven:_com.squareup.okhttp3:okhttp:3.9.1.classpath"/>
    <path refid="library.maven:_com.squareup.okio:okio:1.13.0.classpath"/>
    <path refid="library.maven:_org.json:json:20171018.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-surefire-provider:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-launcher:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-engine:1.0.2.classpath"/>
    <path refid="library.maven:_org.apache.maven.surefire:surefire-api:2.19.1.classpath"/>
    <path refid="library.maven:_org.apache.maven.surefire:common-java5:2.19.1.classpath"/>
    <path refid="library.maven:_org.junit.jupiter:junit-jupiter-api:5.1.0-m1.classpath"/>
    <path refid="library.maven:_org.opentest4j:opentest4j:1.0.0.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-commons:1.1.0-m1.classpath"/>
  </path>
  
  <path id="christmascracker.runtime.module.classpath">
    <pathelement location="${christmascracker.testoutput.dir}"/>
    <pathelement location="${christmascracker.output.dir}"/>
    <path refid="library.json-20171018.classpath"/>
    <path refid="library.maven:_com.google.code.gson:gson:2.8.2.classpath"/>
    <path refid="library.maven:_postgresql:postgresql:9.1-901.jdbc4.classpath"/>
    <path refid="library.maven:_com.squareup.okhttp3:okhttp:3.9.1.classpath"/>
    <path refid="library.maven:_com.squareup.okio:okio:1.13.0.classpath"/>
    <path refid="library.maven:_org.json:json:20171018.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-surefire-provider:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-launcher:1.0.2.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-engine:1.0.2.classpath"/>
    <path refid="library.maven:_org.apache.maven.surefire:surefire-api:2.19.1.classpath"/>
    <path refid="library.maven:_org.apache.maven.surefire:common-java5:2.19.1.classpath"/>
    <path refid="library.maven:_org.junit.jupiter:junit-jupiter-api:5.1.0-m1.classpath"/>
    <path refid="library.maven:_org.opentest4j:opentest4j:1.0.0.classpath"/>
    <path refid="library.maven:_org.junit.platform:junit-platform-commons:1.1.0-m1.classpath"/>
  </path>
  
  
  <patternset id="excluded.from.module.christmascracker">
    <patternset refid="ignored.files"/>
  </patternset>
  
  <patternset id="excluded.from.compilation.christmascracker">
    <patternset refid="excluded.from.module.christmascracker"/>
  </patternset>
  
  <path id="christmascracker.module.sourcepath">
    <dirset dir="${module.christmascracker.basedir}">
      <include name="src/java"/>
      <include name="src/resources"/>
    </dirset>
  </path>
  
  
  <target name="compile.module.christmascracker" depends="compile.module.christmascracker.production,compile.module.christmascracker.tests" description="Compile module ChristmasCracker"/>
  
  <target name="compile.module.christmascracker.production" depends="register.custom.compilers" description="Compile module ChristmasCracker; production classes">
    <mkdir dir="${christmascracker.output.dir}"/>
    <javac2 destdir="${christmascracker.output.dir}" debug="${compiler.debug}" nowarn="${compiler.generate.no.warnings}" memorymaximumsize="${compiler.max.memory}" fork="true" executable="${module.jdk.bin.christmascracker}/javac">
      <compilerarg line="${compiler.args.christmascracker}"/>
      <bootclasspath refid="christmascracker.module.bootclasspath"/>
      <classpath refid="christmascracker.module.production.classpath"/>
      <src refid="christmascracker.module.sourcepath"/>
      <patternset refid="excluded.from.compilation.christmascracker"/>
    </javac2>
    
    <copy todir="${christmascracker.output.dir}">
      <fileset dir="${module.christmascracker.basedir}/src/java">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
      <fileset dir="${module.christmascracker.basedir}/src/resources">
        <patternset refid="compiler.resources"/>
        <type type="file"/>
      </fileset>
    </copy>
  </target>
  
  <target name="compile.module.christmascracker.tests" depends="register.custom.compilers,compile.module.christmascracker.production" description="compile module ChristmasCracker; test classes" unless="skip.tests"/>
  
  <target name="clean.module.christmascracker" description="cleanup module">
    <delete dir="${christmascracker.output.dir}"/>
    <delete dir="${christmascracker.testoutput.dir}"/>
  </target>
</project>